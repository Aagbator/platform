<!-- should this be PUT?  -->
<%= form_tag lesson_contents_path(@lesson_content), method: :post, multipart: true do %>
  <%= label_tag :lesson_content_zipfile, 'Rise 360 zipfile' %>
  <%= file_field_tag :lesson_content_zipfile, :accept => "application/zip" %>
  <input id="state" name='state' type='hidden' value="<%= params[:state] %>" />
  <%= submit_tag "Upload" %>
<% end %>
